    sub [@LamIniBeforeAlef lam_hamza-ar.preAlef.init] @AlefFin' by @AlefFinAfterLamIni;
    # Subtable: Ligatures\Part2b
    sub [@LamMedBeforeAlef] @AlefFin' by @AlefFinAfterLamMed;
} LamAlefLigatures;

