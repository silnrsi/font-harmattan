    lookupflag UseMarkFilteringSet @HamzaKasraFatha;
    sub alef-ar kasra-ar by alef_kasra-ar;
    sub alef-ar kasra-ar.lowered by alef_kasra-ar;
    sub alef-ar fatha-ar by alef_fatha-ar;
} LigaturesForWarsh3;

