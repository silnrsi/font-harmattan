    sub [endofayah-ar endofayah-ar.alt endofayah-ar.altB]' lookup SignWith_3digits_target @DigitsAny' lookup SignDigits_ToSmall_target @DigitsAny' lookup SignDigits_ToSmall_target @DigitsAny' lookup SignDigits_ToSmall_target;
    sub [number-ar year-ar samvat-ar footnotemarker-ar pagenumber-ar numbermark-ar endofayah-ar endofayah-ar.alt endofayah-ar.altB]' lookup SignWith_2digits_target @DigitsAny' lookup SignDigits_ToMedium_target @DigitsAny' lookup SignDigits_ToMedium_target;
