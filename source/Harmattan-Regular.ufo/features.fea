    lookupflag IgnoreMarks;
    # Uniscribe or HarfBuzz 'latn'
    sub [year-ar samvat-ar pagenumber-ar numbermark-ar]' lookup SignWith_4digits_target @DigitsAny' lookup SignDigits_ToMedium_target @DigitsAny' lookup SignDigits_ToMedium_target @DigitsAny' lookup SignDigits_ToMedium_target @DigitsAny' lookup SignDigits_ToMedium_target;
