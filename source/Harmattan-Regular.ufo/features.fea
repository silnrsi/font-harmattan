lookup AllahSubAlef {
    lookupflag 0;
    sub alef-ar by alef-ar.allah;
    sub alef-ar.fina by alef-ar.fina.allah;
} AllahSubAlef;

lookup AllahSubLam1 {
    lookupflag 0;
    sub lam-ar.init by lam-ar.init.allah1;
    sub lam-ar.medi by lam-ar.medi.allah1;
} AllahSubLam1;

lookup AllahSubLam2Heh {
    lookupflag 0;
    sub lam-ar.medi by lam-ar.medi.allah2;
    sub heh-ar.fina by heh-ar.fina.allah;
    sub hehgoal-ar.fina by hehgoal-ar.fina.allah;
} AllahSubLam2Heh;

