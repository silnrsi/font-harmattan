    sub ae-ar hamzaabove-ar by hehHamzaabove-ar;
    sub waw-ar.fina hamzaabove-ar by wawHamzaabove-ar.fina;
    sub yeh-ar.fina hamzaabove-ar by yehHamzaabove-ar.fina;
    sub ae-ar.fina hamzaabove-ar by hehHamzaabove-ar.fina;
    sub yeh-ar.medi hamzaabove-ar by yehHamzaabove-ar.medi;
    sub yeh-ar.init hamzaabove-ar by yehHamzaabove-ar.init;
} ComposeLowHamzaAbove;

