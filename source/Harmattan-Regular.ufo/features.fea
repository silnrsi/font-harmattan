#********************
# Subtending mark positioning
lookup SubtendingMarks {
    lookupflag IgnoreMarks;
    # In Graphite we use mark attachment rules. However, in OpenType positioning digits on 
    # subtending marks has to be done with adjustments rather than attachment since for some 
    # engines the digits will ordered *before* the subtending mark.
    # Additionally, the adjustment value records depend on font face (bold vs regular). Therefore
    # we use feax extentions to calculate the value records based on glyph advancewidths and digit AP coordinates.
    # In each "do" template, the first rule is ordered for Uniscribe or HarfBuzz 'latin', the second for HarfBuzz 'arab'
    # U+06DD End of Ayah
            pos @AyahAny @DigitsAnySmall' <-1754 0 -494 0> @DigitsAnySmall' <-1260 0 -494 0> @DigitsAnySmall' <-766 0 -494 0>;
        pos @DigitsAnySmall' <-766 0 -494 0> @DigitsAnySmall' <-1260 0 -494 0> @DigitsAnySmall' <-1754 0 -494 0> @AyahAny;

            pos @AyahAny @DigitsAnyMedium' <-1686 0 -690 0> @DigitsAnyMedium' <-996 0 -690 0>;
        pos @DigitsAnyMedium' <-996 0 -690 0> @DigitsAnyMedium' <-1686 0 -690 0> @AyahAny;

            # U+0600 Number sign
        pos @AyahAny @DigitsAnyMedium' <-1346 0 -690 0>;
        pos @DigitsAnyMedium' <-1346 0 -690 0> @AyahAny;

            pos number-ar.3 @DigitsAnyMedium' <-2829 0 -690 0> @DigitsAnyMedium' <-2139 0 -690 0> @DigitsAnyMedium' <-1449 0 -690 0>;
        pos @DigitsAnyMedium' <-1449 0 -690 0> @DigitsAnyMedium' <-2139 0 -690 0> @DigitsAnyMedium' <-2829 0 -690 0> number-ar.3;

            pos number-ar.2 @DigitsAnyMedium' <-2146 0 -690 0> @DigitsAnyMedium' <-1456 0 -690 0>;
        pos @DigitsAnyMedium' <-1456 0 -690 0> @DigitsAnyMedium' <-2146 0 -690 0> number-ar.2;

            # U+0601 Year sign
        pos number-ar @DigitsAnyMedium' <-1539 0 -690 0>;
        pos @DigitsAnyMedium' <-1539 0 -690 0> number-ar;

            pos year-ar.4 @DigitsAnyMedium' <-3130 0 -690 0> @DigitsAnyMedium' <-2440 0 -690 0> @DigitsAnyMedium' <-1750 0 -690 0> @DigitsAnyMedium' <-1060 0 -690 0>;
