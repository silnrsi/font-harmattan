    pos @YehBarreeMarkLower @MarksBelow' <0 -300 0 0> yehbarree-ar.fina;
    pos @YehBarreeMarkLower @MarksAbove @MarksBelow' <0 -300 0 0> yehbarree-ar.fina;
    pos @YehBarreeMarkLower @MarksBelow' <0 -300 0 0> @MarksAbove yehbarree-ar.fina;
} MarkKern;

#********************
# Subtending mark positioning
lookup SubtendingMarks {
    lookupflag IgnoreMarks;
    # In Graphite we use mark attachment rules. However, in OpenType positioning digits on 
    # subtending marks has to be done with adjustments rather than attachment since for some 
    # engines the digits will ordered *before* the subtending mark.
    # Additionally, the adjustment value records depend on font face (bold vs regular). Therefore
    # we use feax extentions to calculate the value records based on glyph advancewidths and digit AP coordinates.
    # In each "do" template, the first rule is ordered for Uniscribe or HarfBuzz 'latin', the second for HarfBuzz 'arab'
    # U+06DD End of Ayah
            pos @AyahAny @DigitsAnySmall' <-1794 0 -500 0> @DigitsAnySmall' <-1294 0 -500 0> @DigitsAnySmall' <-794 0 -500 0>;
        pos @DigitsAnySmall' <-794 0 -500 0> @DigitsAnySmall' <-1294 0 -500 0> @DigitsAnySmall' <-1794 0 -500 0> @AyahAny;

            pos @AyahAny @DigitsAnyMedium' <-1726 0 -750 0> @DigitsAnyMedium' <-976 0 -750 0>;
        pos @DigitsAnyMedium' <-976 0 -750 0> @DigitsAnyMedium' <-1726 0 -750 0> @AyahAny;

            # U+0600 Number sign
        pos @AyahAny @DigitsAnyMedium' <-1386 0 -750 0>;
        pos @DigitsAnyMedium' <-1386 0 -750 0> @AyahAny;

            pos number-ar.3 @DigitsAnyMedium' <-2875 0 -750 0> @DigitsAnyMedium' <-2125 0 -750 0> @DigitsAnyMedium' <-1375 0 -750 0>;
        pos @DigitsAnyMedium' <-1375 0 -750 0> @DigitsAnyMedium' <-2125 0 -750 0> @DigitsAnyMedium' <-2875 0 -750 0> number-ar.3;

            pos number-ar.2 @DigitsAnyMedium' <-2225 0 -750 0> @DigitsAnyMedium' <-1475 0 -750 0>;
        pos @DigitsAnyMedium' <-1475 0 -750 0> @DigitsAnyMedium' <-2225 0 -750 0> number-ar.2;

            # U+0601 Year sign
        pos number-ar @DigitsAnyMedium' <-1625 0 -750 0>;
        pos @DigitsAnyMedium' <-1625 0 -750 0> number-ar;

            pos year-ar.4 @DigitsAnyMedium' <-3165 0 -750 0> @DigitsAnyMedium' <-2415 0 -750 0> @DigitsAnyMedium' <-1665 0 -750 0> @DigitsAnyMedium' <-915 0 -750 0>;
        pos @DigitsAnyMedium' <-915 0 -750 0> @DigitsAnyMedium' <-1665 0 -750 0> @DigitsAnyMedium' <-2415 0 -750 0> @DigitsAnyMedium' <-3165 0 -750 0> year-ar.4;

            pos year-ar.3 @DigitsAnyMedium' <-2455 0 -750 0> @DigitsAnyMedium' <-1705 0 -750 0> @DigitsAnyMedium' <-955 0 -750 0>;
        pos @DigitsAnyMedium' <-955 0 -750 0> @DigitsAnyMedium' <-1705 0 -750 0> @DigitsAnyMedium' <-2455 0 -750 0> year-ar.3;

            pos year-ar.2 @DigitsAnyMedium' <-1765 0 -750 0> @DigitsAnyMedium' <-1015 0 -750 0>;
        pos @DigitsAnyMedium' <-1015 0 -750 0> @DigitsAnyMedium' <-1765 0 -750 0> year-ar.2;

            # U+0602 Footnote Marker
        pos year-ar @DigitsAnyMedium' <-1097 0 -750 0>;
        pos @DigitsAnyMedium' <-1097 0 -750 0> year-ar;

            pos footnotemarker-ar.2 @DigitsAnyMedium' <-1677 0 -750 0> @DigitsAnyMedium' <-927 0 -750 0>;
        pos @DigitsAnyMedium' <-927 0 -750 0> @DigitsAnyMedium' <-1677 0 -750 0> footnotemarker-ar.2;

            # U+0603 Page Number
        pos footnotemarker-ar @DigitsAnyMedium' <-1012 0 -750 0>;
        pos @DigitsAnyMedium' <-1012 0 -750 0> footnotemarker-ar;

            pos pagenumber-ar.4 @DigitsAnyMedium' <-4052 0 -750 0> @DigitsAnyMedium' <-3302 0 -750 0> @DigitsAnyMedium' <-2552 0 -750 0> @DigitsAnyMedium' <-1802 0 -750 0>;
        pos @DigitsAnyMedium' <-1802 0 -750 0> @DigitsAnyMedium' <-2552 0 -750 0> @DigitsAnyMedium' <-3302 0 -750 0> @DigitsAnyMedium' <-4052 0 -750 0> pagenumber-ar.4;

            pos pagenumber-ar.3 @DigitsAnyMedium' <-3372 0 -750 0> @DigitsAnyMedium' <-2622 0 -750 0> @DigitsAnyMedium' <-1872 0 -750 0>;
        pos @DigitsAnyMedium' <-1872 0 -750 0> @DigitsAnyMedium' <-2622 0 -750 0> @DigitsAnyMedium' <-3372 0 -750 0> pagenumber-ar.3;

            pos pagenumber-ar.2 @DigitsAnyMedium' <-2612 0 -750 0> @DigitsAnyMedium' <-1862 0 -750 0>;
        pos @DigitsAnyMedium' <-1862 0 -750 0> @DigitsAnyMedium' <-2612 0 -750 0> pagenumber-ar.2;

            # U+0604 Samvat sign
        pos pagenumber-ar @DigitsAnyMedium' <-2022 0 -750 0>;
        pos @DigitsAnyMedium' <-2022 0 -750 0> pagenumber-ar;

            pos samvat-ar.4 @DigitsAnyMedium' <-3867 0 -750 0> @DigitsAnyMedium' <-3117 0 -750 0> @DigitsAnyMedium' <-2367 0 -750 0> @DigitsAnyMedium' <-1617 0 -750 0>;
        pos @DigitsAnyMedium' <-1617 0 -750 0> @DigitsAnyMedium' <-2367 0 -750 0> @DigitsAnyMedium' <-3117 0 -750 0> @DigitsAnyMedium' <-3867 0 -750 0> samvat-ar.4;

            pos samvat-ar.3 @DigitsAnyMedium' <-3185 0 -750 0> @DigitsAnyMedium' <-2435 0 -750 0> @DigitsAnyMedium' <-1685 0 -750 0>;
        pos @DigitsAnyMedium' <-1685 0 -750 0> @DigitsAnyMedium' <-2435 0 -750 0> @DigitsAnyMedium' <-3185 0 -750 0> samvat-ar.3;

            pos samvat-ar.2 @DigitsAnyMedium' <-2504 0 -750 0> @DigitsAnyMedium' <-1754 0 -750 0>;
        pos @DigitsAnyMedium' <-1754 0 -750 0> @DigitsAnyMedium' <-2504 0 -750 0> samvat-ar.2;

            # U+0605 Number mark
        pos samvat-ar @DigitsAnyMedium' <-1882 0 -750 0>;
        pos @DigitsAnyMedium' <-1882 0 -750 0> samvat-ar;

            pos numbermark-ar.4 @DigitsAnyMedium' <-3267 0 -750 0> @DigitsAnyMedium' <-2517 0 -750 0> @DigitsAnyMedium' <-1767 0 -750 0> @DigitsAnyMedium' <-1017 0 -750 0>;
        pos @DigitsAnyMedium' <-1017 0 -750 0> @DigitsAnyMedium' <-1767 0 -750 0> @DigitsAnyMedium' <-2517 0 -750 0> @DigitsAnyMedium' <-3267 0 -750 0> numbermark-ar.4;

            pos numbermark-ar.3 @DigitsAnyMedium' <-2507 0 -750 0> @DigitsAnyMedium' <-1757 0 -750 0> @DigitsAnyMedium' <-1007 0 -750 0>;
        pos @DigitsAnyMedium' <-1007 0 -750 0> @DigitsAnyMedium' <-1757 0 -750 0> @DigitsAnyMedium' <-2507 0 -750 0> numbermark-ar.3;

            pos numbermark-ar.2 @DigitsAnyMedium' <-1773 0 -750 0> @DigitsAnyMedium' <-1023 0 -750 0>;
        pos @DigitsAnyMedium' <-1023 0 -750 0> @DigitsAnyMedium' <-1773 0 -750 0> numbermark-ar.2;

            pos numbermark-ar @DigitsAnyMedium' <-1033 0 -750 0>;
        pos @DigitsAnyMedium' <-1033 0 -750 0> numbermark-ar;

            

    # For reference: This was the way we did it in Harmattan 1.0 :
    # Uniscribe or HarfBuzz 'latn'
    # HarfBuzz 'arab'
} SubtendingMarks;

