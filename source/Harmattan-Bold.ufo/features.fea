lookup ShaddaAlefLigatures {
    lookupflag UseMarkFilteringSet @ShaddaLigatureFilter;
            # not in Harmattan    sub lam-ar.medi.allah2 shadda-ar'    alefabove-ar'  by  shadda_alef-ar;
        # end do - NOT in Harmattan
