# Subtending mark positioning
lookup SubtendingMarks {
    lookupflag IgnoreMarks;
    # In Graphite we use mark attachment rules. However, in OpenType positioning digits on 
    # subtending marks has to be done with adjustments rather than attachment since for some 
    # engines the digits will ordered *before* the subtending mark.
    # Additionally, the adjustment value records depend on font face (bold vs regular). Therefore
    # we use feax extentions to calculate the value records based on glyph advancewidths and digit AP coordinates.
    # In each "do" template, the first rule is ordered for Uniscribe or HarfBuzz 'latin', the second for HarfBuzz 'arab'
    # U+06DD End of Ayah
            pos @AyahAny @DigitsAnySmall' <-1794 0 -500 0> @DigitsAnySmall' <-1294 0 -500 0> @DigitsAnySmall' <-794 0 -500 0>;
        pos @DigitsAnySmall' <-794 0 -500 0> @DigitsAnySmall' <-1294 0 -500 0> @DigitsAnySmall' <-1794 0 -500 0> @AyahAny;

            pos @AyahAny @DigitsAnyMedium' <-1726 0 -750 0> @DigitsAnyMedium' <-976 0 -750 0>;
        pos @DigitsAnyMedium' <-976 0 -750 0> @DigitsAnyMedium' <-1726 0 -750 0> @AyahAny;

            # U+0600 Number sign
        pos @AyahAny @DigitsAnyMedium' <-1386 0 -750 0>;
        pos @DigitsAnyMedium' <-1386 0 -750 0> @AyahAny;

            pos number-ar.3 @DigitsAnyMedium' <-2830 0 -750 0> @DigitsAnyMedium' <-2080 0 -750 0> @DigitsAnyMedium' <-1330 0 -750 0>;
        pos @DigitsAnyMedium' <-1330 0 -750 0> @DigitsAnyMedium' <-2080 0 -750 0> @DigitsAnyMedium' <-2830 0 -750 0> number-ar.3;

            pos number-ar.2 @DigitsAnyMedium' <-2180 0 -750 0> @DigitsAnyMedium' <-1430 0 -750 0>;
        pos @DigitsAnyMedium' <-1430 0 -750 0> @DigitsAnyMedium' <-2180 0 -750 0> number-ar.2;

            # U+0601 Year sign
        pos number-ar @DigitsAnyMedium' <-1580 0 -750 0>;
        pos @DigitsAnyMedium' <-1580 0 -750 0> number-ar;

            pos year-ar.4 @DigitsAnyMedium' <-3120 0 -750 0> @DigitsAnyMedium' <-2370 0 -750 0> @DigitsAnyMedium' <-1620 0 -750 0> @DigitsAnyMedium' <-870 0 -750 0>;
        pos @DigitsAnyMedium' <-870 0 -750 0> @DigitsAnyMedium' <-1620 0 -750 0> @DigitsAnyMedium' <-2370 0 -750 0> @DigitsAnyMedium' <-3120 0 -750 0> year-ar.4;

            pos year-ar.3 @DigitsAnyMedium' <-2410 0 -750 0> @DigitsAnyMedium' <-1660 0 -750 0> @DigitsAnyMedium' <-910 0 -750 0>;
        pos @DigitsAnyMedium' <-910 0 -750 0> @DigitsAnyMedium' <-1660 0 -750 0> @DigitsAnyMedium' <-2410 0 -750 0> year-ar.3;

            pos year-ar.2 @DigitsAnyMedium' <-1720 0 -750 0> @DigitsAnyMedium' <-970 0 -750 0>;
        pos @DigitsAnyMedium' <-970 0 -750 0> @DigitsAnyMedium' <-1720 0 -750 0> year-ar.2;

            # U+0602 Footnote Marker
        pos year-ar @DigitsAnyMedium' <-1052 0 -750 0>;
        pos @DigitsAnyMedium' <-1052 0 -750 0> year-ar;

            pos footnotemarker-ar.2 @DigitsAnyMedium' <-1632 0 -750 0> @DigitsAnyMedium' <-882 0 -750 0>;
        pos @DigitsAnyMedium' <-882 0 -750 0> @DigitsAnyMedium' <-1632 0 -750 0> footnotemarker-ar.2;

            # U+0603 Page Number
